// Initialize main navigation menu and menu behaviors
function setupNav() {

	jQuery('#main-menu__items').smartmenus({
		subIndicatorsPos: 'append',
		subMenusMinWidth: '100%',
		bottomToTopSubMenus: false
	});

	// Menu behaviors 
	var $mainMenuState   = jQuery('#main-menu__state'),
		$mainMenuOverlay = jQuery('#main-menu-overlay');
	if ($mainMenuState.length) {
		$mainMenuState.change(function(e) {
			var $menu = jQuery('#main-menu__items');
			if (this.checked) {
				$menu.hide()
					.css('right', '-80%')
					.show()
					.animate({ right: '0%' }, 250, 'swing', function() { 
						$menu.css('display', ''); 
					});
				$mainMenuOverlay.show();
			} else {
				$menu.show()
					.css('right', '0')
					.animate({ right: "-80%" }, 250, 'swing', function() { 
						$menu.css('display', ''); 
					});
				$mainMenuOverlay.hide();
			}
		});
		// Hide mobile menu when window is unloaded 
		// (prevents the menu from being shown on refresh)
		jQuery(window).bind('beforeunload unload', function() {
			if ($mainMenuState[0].checked) {
				$mainMenuState[0].click();
			}
		});
	}
	$mainMenuOverlay.click(function(e) {
		$mainMenuState.prop('checked', false).change();
	});
}

function getCartText() {
	var $cart = jQuery('.cart__text');

	jQuery.ajax({
		type: 'POST',
		url: ajaxurl,
		data: {
			'action' : 'afx_cart_text'
		},
		dataType: 'json',
		encode: true
	}).done(function(data) {
		if (data.success) {
			$cart.html(data.cart_text);
		} else {
			console.log('Failed to update cart text');
		}
	});
}

// Click handler for vote button
function voteBtnHandler(e) {

	var $form    = jQuery(this).closest('.afx-vote-form'),
		$message = jQuery('.upcoming-item__message', $form),
		$btn     = jQuery('.afx-vote-btn', $form),
		$votes   = jQuery('.afx-votes', $form),
		formData = {
			'pid'    : jQuery('input[name="afx_vote_pid"]', $form).val(),
			'hash'   : jQuery('input[name="afx_vote_hash"]', $form).val(),
			'action' : 'afx_vote'
		}
	
	jQuery.ajax({
		type: 'POST',
		url: ajaxurl,
		data: formData,
		dataType: 'json',
		encode: true
	}).done(function(data) {
		if (data.success) {
			// Change the icon
			if (data.direction == 'up') {
				$btn.html('<span class="fas fa-heart fa-beat"></span>');
			} else {
				$btn.html('<span class="far fa-heart"></span>');
			}
			// Set the vote count
			if (data.votes == 0) {
				data.votes = 'Be the first to vote for this.';
			}
			$votes.html(data.votes);
		} else {
			if (data.error == 'invalid_request') {
				console.log('Vote failure (invalid request)');
			} else {
				console.log('Vote failure (unspecified)');
			}
		}
	});

	e.preventDefault();

};

// Slightly modified version of kit 
function requestKitBtnHandler(e) {

	var $form    = jQuery(this).closest('.afx-product-vote-form'),
		$message = jQuery('.product__message', $form),
		$btn     = jQuery('.afx-product-vote-btn', $form),
		$icon    = jQuery('.afx-product-vote-btn-icon', $btn),
		$votes   = jQuery('.product__message', $form),
		formData = {
			'pid'    : jQuery('input[name="afx_vote_pid"]', $form).val(),
			'hash'   : jQuery('input[name="afx_vote_hash"]', $form).val(),
			'action' : 'afx_vote'
		}

	$icon.html('<span class="fas fa-circle-notch fa-spin"></span>');
	
	jQuery.ajax({
		type: 'POST',
		url: ajaxurl,
		data: formData,
		dataType: 'json',
		encode: true
	}).done(function(data) {
		if (data.success) {
			// Change the icon
			if (data.direction == 'up') {
				$btn.html('<span class="afx-product-vote-btn-icon"><span class="fas fa-heart fa-beat"></span></span> Requested');
			} else {
				$btn.html('<span class="afx-product-vote-btn-icon"><span class="far fa-heart"></span></span> Request Kit');
			}
			// Set the vote count
			if (data.votes == 1) {
				$votes.removeClass('product__message--hidden').html('<span class="product__message__votes">1</span> person is interested.');
			} else if (data.votes > 1) {
				$votes.removeClass('product__message--hidden').html('<span class="product__message__votes">' + data.votes + '</span> people are interested.');
			} else {
				$votes.addClass('product__message--hidden');
			}
		} else {
			if (data.error == 'invalid_request') {
				console.log('Vote failure (invalid request)');
			} else {
				console.log('Vote failure (unspecified)');
			}
		}
	});

	e.preventDefault();

};


// Click handler for email subscription button
function subscribeBtnHandler(e) {

	var $form       = jQuery(this).closest('.afx-email-form'),
		$message    = jQuery('.afx_email_submission_message', $form),
		$emailInput = jQuery('input[name="afx_email_address"]', $form),
		$btn        = jQuery('.afx-email-btn', $form),
		formData    = {
			'email'  : $emailInput.val(),
			'hash'   : jQuery('input[name="afx_email_hash"]', $form).val(),
