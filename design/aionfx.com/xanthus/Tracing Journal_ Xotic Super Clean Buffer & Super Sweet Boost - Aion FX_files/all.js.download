// Handle the checkbox dependencies
function productCategoryChange() {

	var $this     = jQuery(this),
		$outer    = $this.closest('.widget__outer'),
		$reset    = jQuery('.widget__reset', $outer),
		$allInput = jQuery('input[value=0], input[value=any]', $outer),
		$inputs   = jQuery('input[type=checkbox]', $outer);

	if ($inputs.length) {

		// This will only run on the two groups of checkboxes
		var $allButAll = $inputs.filter(':not([value=0])'),
			$checked   = $inputs.filter(':checked');

