// Hook from bp:xs, bp:sm, bp:md
function mobilizeInteriorNav() {
	// Verify the existence of a sidebar menu on this page.
	// Note that for pages, the sidebar menu can also be called `#secondary`,
	// in which case we do not want to transform into the mobile version since
	// it's basically a throwaway/placeholder menu.
	if (jQuery('#interior').length) {
		transformInteriorNav('dropdown');
	} else if (jQuery('#widgets').length) {
		transformWidgets('dropdown');
	}
}

// Hook from bp:lg, bp:xl
function deMobilizeInteriorNav() {
	if (jQuery('#interior').length) {
		transformInteriorNav('sidebar');
	} else if (jQuery('#widgets').length) {
		transformWidgets('sidebar');
	}
}


// Transform sidebar navigation into mobile dropdown and back.
function transformInteriorNav(type) {

	if (typeof type === 'undefined') {
		console.log('No transformation type specified.');
		return;
	}

	var $intMenu      = jQuery('#interior'),
		parentId      = $intMenu.parent().attr('id'),
		$sidenavTitle = jQuery('.sidenav__title', $intMenu);

	if (parentId == 'sidebar' && type == 'dropdown') {
		// Convert to dropdown-type
		$intMenu.appendTo('#sidenav__anchor').addClass('sidebar--dropdown');
		// Attach events to dropdown
		$sidenavTitle.on('click', interiorNavDropdown);
	} else if (parentId == 'sidenav__anchor' && type == 'sidebar') {
		// Convert to sidebar-style
		$intMenu.appendTo('#sidebar').removeClass('sidebar--dropdown');
		$sidenavTitle.off('click', interiorNavDropdown);
	}
}

// Click handler for mobile interior nav dropdown functionality
function interiorNavDropdown() {
	var $this  = jQuery(this),
		$menu  = $this.next('.sidenav__wrap'),
		$outer = $this.parent('.sidenav__outer');

	if (!$menu.length || !$outer.length) {
		return;
	}

	if ($outer.hasClass('expanded')) {
		$outer.removeClass('expanded');
		$menu.show().slideUp(250, function() { 
			$menu.css('display', ''); 
		});
	} else {
		$outer.addClass('expanded');
		$menu.hide().slideDown(250, function() { 
			$menu.css('display', ''); 
		});
	}
}

// Transform news widgets into mobile dropdown and back.

var mobileTransformed = false,
	desktopTransformed = false;

function transformWidgets(type) {

	if (typeof type === 'undefined') {
		console.log('No transformation type specified.');
		return;
	}

	var $widgetContainer = jQuery('#widgets'),
		parentId         = $widgetContainer.parent().attr('id'),
		$widgets         = jQuery('.widget', $widgetContainer);

	// Clear out any expanded states when going between breakpoints
	$widgets.removeClass('widget--expanded');

	if (type == 'dropdown' && mobileTransformed == false) { // Convert container to dropdown-type

		mobileTransformed = true;
		desktopTransformed = false;

		// If there's a #widgets_anchor on the page, move it there
		var $widgetsAnchor = jQuery('#widgets__anchor');
		if (parentId == 'sidebar' && $widgetsAnchor.length) {
			$widgetContainer.appendTo('#widgets__anchor');
		}
		// Attach events to dropdown
		$widgets.each(function() {
			var $this  = jQuery(this),
				$title = jQuery('.widget__title', $this),
				$inner = jQuery('.widget__inner', $this);
			if ($inner.length && !$this.hasClass('widget--no-convert')) {
				$this.addClass('widget--dropdown');
				$title.on('click', widgetDropdown);
			}
		});
	} else if (type == 'sidebar' && desktopTransformed == false) { // Convert container to sidebar-style

		desktopTransformed = true;
		mobileTransformed = false;

		// If it's currently inside #widgets_anchor, move it back to the sidebar
		var $widgetSidebar = jQuery('#sidebar');
		if (parentId == 'widgets__anchor' && $widgetSidebar.length) {
			$widgetContainer.appendTo('#sidebar');			
		}

		$widgets.each(function() {
			var $this  = jQuery(this),
				$title = jQuery('.widget__title', $this),
				$inner = jQuery('.widget__inner', $this);
			if ($inner.length && !$this.hasClass('widget--no-convert')) {
				$this.removeClass('widget--dropdown');
				$title.off('click', widgetDropdown);
			}
		});
	}
}

// Click handler for widget dropdown functionality
function widgetDropdown() {
	var $this   = jQuery(this), // .widget__title
		$widget = $this.parent(),
		$inner  = jQuery('.widget__inner', $widget);

	if (!$inner.length) {
		return;
