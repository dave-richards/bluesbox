			if (!$checked.length) {
				$allInput.prop('checked', true);
			} else if ($checked.length && $checked.length != $allButAll.length) {
				$allInput.prop('checked', false);
			} else if ($checked.length == $allButAll.length) {
				$allInput.prop('checked', true);
				$allButAll.prop('checked', false);
			}
		} else {
			// Clicked item is the All checkbox
			if ($allInput.prop('checked') == true) {
				// If All is checked, uncheck all others
				$allButAll.prop('checked', false);
			} else {
				// Prevent the All checkbox from being unchecked directly
				$allInput.prop('checked', true);
			}
		}
