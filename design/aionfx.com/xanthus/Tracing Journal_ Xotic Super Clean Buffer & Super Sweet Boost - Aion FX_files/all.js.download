var footerMenuAnimating = false;
function footerMenuExpand(e) {

	e.preventDefault();

	// Cancel if there's another animation happening
	if (footerMenuAnimating) {
		return;
	}
	var $footerHeader    = jQuery(this),
		$footerMenuOuter = $footerHeader.closest('.footer-collapse__outer'),
		$footerMenu      = jQuery('.footer-collapse', $footerMenuOuter).first();

	if ($footerMenuOuter.hasClass('footer-collapse__outer--collapsed')) {
		footerMenuAnimating = true;
		// Get natural height
		$footerMenu.css('height', 'auto');
		var footerMenuHeight = $footerMenu.outerHeight();
		$footerMenu.css('height', 0);
		$footerMenuOuter.removeClass('footer-collapse__outer--collapsed');
		$footerMenu.animate({'height' : footerMenuHeight + 'px'}, 250, 'swing', function() {
			$footerMenu.css('height', '');
			footerMenuAnimating = false;
		});
	} else {
		footerMenuAnimating = true;
		$footerMenuOuter.addClass('footer-collapse__outer--collapsed');
		$footerMenu.css('height', 'auto').animate({'height': 0}, 250, 'swing', function() {
			$footerMenu.css('height', '');
			footerMenuAnimating = false;
		});
	}
}

